global @using Core.ViewModels;
@model List<ProductVM>
@{
}

<table>
    <thead>
        <th>#</th>
        <th>Name</th>
        <th>Category</th>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.CategoryId</td>
            </tr>
        }
    </tbody>
</table>


